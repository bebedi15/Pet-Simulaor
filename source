Spawn.Name = "Spawn"


Spawn.Parent = main


Spawn.BackgroundColor3 = Color3.fromRGB(74, 74, 74)


Spawn.Position = UDim2.new(0.0681818202, 0, 0.149943829, 0)


Spawn.Size = UDim2.new(0, 228, 0, 24)


Spawn.Font = Enum.Font.SourceSans


Spawn.Text = "Spawn"


Spawn.TextColor3 = Color3.fromRGB(0, 0, 0)


Spawn.TextSize = 14.000


Spawn.MouseButton1Down:Connect(function()


	function teleportTO(placeCFrame)


		local plyr = game.Players.LocalPlayer


		if plyr.Character then


			plyr.Character.HumanoidRootPart.CFrame = placeCFrame


		end


	end


	teleportTO(game:GetService("Workspace")["__MAP"].Areas.Spawn.Part.CFrame)


end)



