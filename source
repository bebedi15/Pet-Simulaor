--Properties:



ScreenGui.Parent = game.CoreGui



main.Name = "main"


main.Parent = ScreenGui


main.Active = true


main.BackgroundColor3 = Color3.fromRGB(85, 85, 85)


main.Position = UDim2.new(0.775130332, 0, 0.246625766, 0)


main.Size = UDim2.new(0, 264, 0, 576)


main.Active = true


main.Draggable = true



Titel.Name = "Titel"


Titel.Parent = main


